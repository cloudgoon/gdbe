<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
  
  <servlet>
    <servlet-name>GDBEEditorServlet</servlet-name>
    <servlet-class>com.google.gdbe.server.GDBEEditorServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>GDBEExplorerServlet</servlet-name>
    <servlet-class>com.google.gdbe.server.GDBEExplorerServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>GDataServlet</servlet-name>
    <servlet-class>com.google.gdbe.server.GDataServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>GDBEEditorServlet</servlet-name>
    <url-pattern>/docs/*</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>GDBEExplorerServlet</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>GDataServlet</servlet-name>
    <url-pattern>/com.google.gdbe.editor.GDBEEditor/data</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>GDataServlet</servlet-name>
    <url-pattern>/com.google.gdbe.explorer.GDBEExplorer/data</url-pattern>
  </servlet-mapping>
  
</web-app>
